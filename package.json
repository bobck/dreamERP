{
  "name": "dream-erp",
  "version": "1.0.0",
  "description": "Taxi fleet ERP",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "taxi",
    "erp",
    "bolt",
    "taxy-fy",
    "fleet",
    "react"
  ],
  "author": "Vitaly Chaykovskiy <bobckx10@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^5.8.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "csv-stringify": "^5.6.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "googleapis": "^88.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.9",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12"
  }
}
